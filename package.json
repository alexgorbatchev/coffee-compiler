{
  "name": "coffee-compiler",
  "main": "./coffee-compiler.js",
  "version": "0.3.0",
  "description": "A handy CoffeeScript compiler from a series of compilers that have the same signature.",
  "keywords": ["coffeesciprt", "coffee", "compiler"],
  "repository": {
    "type": "git",
    "url": "https://github.com/alexgorbatchev/coffee-compiler.git"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha test/*.spec.coffee",
    "pretest": "./node_modules/.bin/coffee --bare --compile *.coffee"
  },
  "peerDependencies": {
    "coffee-script": ">=1.x.x"
  },
  "devDependencies": {
    "coffee-script": "~1.6.3",
    "chai": "~1.6.0",
    "mocha": "~1.10.0",
    "fibrous": "~0.3.3"
  }
}
